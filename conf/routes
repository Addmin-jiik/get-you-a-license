# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET     /                           controllers.Main.index

GET     /_github_oauth_callback     controllers.Main.gitHubOauthCallback(code: String, state: String)

GET     /:org                       controllers.Main.gitHubOrg(org: String, accessToken: String)

POST    /license_pull_request       controllers.Main.licensePullRequest(org: String, repo: String, license: String, accessToken: String)

GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
